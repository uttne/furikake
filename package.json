{
    "name": "test-data-util-extensions",
    "displayName": "test-data-util-extensions",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:test-data-util-extensions.replaceUUIDView",
        "onView:test-data-util-extensions.changeTimezoneView",
        "onCommand:test-data-util-extensions.replaceUUID",
        "onCommand:test-data-util-extensions.changeTimeZone"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "test-data-util-extensions-launcher",
                    "title": "Test data util",
                    "icon": "media/icon.svg"
                }
            ]
        },
        "views": {
            "test-data-util-extensions-launcher": [
                {
                    "type": "webview",
                    "id": "test-data-util-extensions.replaceUUIDView",
                    "name": "Replace UUID"
                },
                {
                    "type": "webview",
                    "id": "test-data-util-extensions.changeTimezoneView",
                    "name": "Change Timezone"
                }
            ]
        },
        "commands": [
            {
                "command": "test-data-util-extensions.replaceUUID",
                "title": "Replace UUID"
            },
            {
                "command": "test-data-util-extensions.changeTimeZone",
                "title": "Change Timezone"
            }
        ],
        "menus": {}
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/uuid": "^8.3.4",
        "@types/vscode": "^1.70.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vscode/test-electron": "^2.1.5",
        "eslint": "^8.20.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "dayjs": "^1.11.5",
        "uuid": "^8.3.2"
    }
}
